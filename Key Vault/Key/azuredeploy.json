{
  "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
	  "Name": {
      "type": "string",
      "metadata": {
        "description": "Name of the Key Vault key"
      },	
      "KetValutName": {
        "type": "string",
        "metadata": {
          "description": "Name of the Key Vault key"
      }	,
      "Options": {
        "type": "string",
        "metadata": {
          "description": "Options"
      }	,
      "KeyType": {
        "type": "string",
        "metadata": {
          "description": "KeyType"
      }	,
      "RSAKeySize": {
        "type": "int",
        "metadata": {
          "description": "RSAKeySize"
      }	
},
  "variables": {},
  "resources": {
      "type":"Microsoft.KeyVault/vaults/keys",
       "name":"[concat(parameters('KetValutName'), '/', parameters('Name'))]",  
       "KeyType":"[parameters('KeyType')]",  
       "Options":"[parameters('Options')]",  
       "KeyType":"[parameters('KeyType')]",  
       "RSAKeySize":"[parameters('RSAKeySize')]",  
       "apiVersion":"2015-06-01",
        "properties":{
          "contentType":"text/plain",
          "value":"ThisIpsemIsSecret"
        },
        "dependsOn":[
          "[resourceId('Microsoft.KeyVault/vaults',[parameters('KetValutName')]])]"
        ]
  },
  "outputs": {}
}
